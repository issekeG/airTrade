{% extends 'base.html.twig' %}

{% block title %}AircraftManufacturer index{% endblock %}

{% block body %}
    <h1>AircraftManufacturer index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for aircraft_manufacturer in aircraft_manufacturers %}
            <tr>
                <td>{{ aircraft_manufacturer.id }}</td>
                <td>{{ aircraft_manufacturer.name }}</td>
                <td>
                    <a href="{{ path('app_aircraft_manufacturer_show', {'id': aircraft_manufacturer.id}) }}">show</a>
                    <a href="{{ path('app_aircraft_manufacturer_edit', {'id': aircraft_manufacturer.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_aircraft_manufacturer_new') }}">Create new</a>
{% endblock %}
